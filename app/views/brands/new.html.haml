%li.list-group-item
  %a.btn.btn-default.btn-block{:href => manage_brands_url}
    = "Back"

%li.list-group-item
  %h4= "Add Brand"

%li.list-group-item
  = render partial: "errMsg", locals: { errMsg: @errMsg }

  = form_for(:brand, url: brands_path, method: :post, :html => {:class => "form-horizontal"}) do |f|
    = f.text_field :name, :placeholder => "Name", :class => "form-control"
    %br
    = f.submit "Submit", :class => "btn btn-default"